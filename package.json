{
  "name": "@folio/notes",
  "version": "5.0.0",
  "description": "Note types manager",
  "repository": "",
  "main": "src/index.js",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./dist",
    "test-int": "stripes test nightmare --run demo --show",
    "test": "stripes test karma",
    "lint": "eslint ./ && stylelint \"src/**/*.css\"",
    "eslint": "eslint ./",
    "stylelint": "stylelint \"src/**/*.css\""
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.8.1",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^5.0.0",
    "@folio/stripes": "^5.0.0",
    "@folio/stripes-cli": "^1.18.0",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^6.2.1",
    "inflected": "^2.0.4",
    "miragejs": "^0.1.40",
    "mocha": "^5.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0",
    "sinon": "^6.3.4",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-junit-formatter": "^0.2.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^5.0.0",
    "react": "*",
    "react-intl": "^4.5.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-notes.meta.title",
    "route": "/notes",
    "okapiInterfaces": {
      "notes": "2.0"
    },
    "queryResource": "query",
    "permissionSets": [
      {
        "permissionName": "module.notes.enabled",
        "displayName": "UI: ui-notes module is enabled",
        "visible": false
      },
      {
        "permissionName": "settings.notes.enabled",
        "displayName": "Settings (notes): display list of settings pages",
        "subPermissions": [
          "settings.enabled",
          "note.types.allops"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.view",
        "displayName": "Notes: Can view a note",
        "subPermissions": [
          "note.types.collection.get",
          "notes.item.get",
          "notes.collection.get",
          "notes.collection.get.by.status",
          "notes.domain.all",
          "module.notes.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.create",
        "displayName": "Notes: Can create a note",
        "subPermissions": [
          "notes.item.post",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.edit",
        "displayName": "Notes: Can edit a note",
        "subPermissions": [
          "notes.item.put",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.delete",
        "displayName": "Notes: Can delete a note",
        "subPermissions": [
          "notes.item.delete",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.assign-unassign",
        "displayName": "Notes: Can assign and unassign a note",
        "subPermissions": [
          "note.links.collection.put",
          "ui-notes.item.view"
        ],
        "visible": true
      }
    ]
  }
}
