{
  "name": "@folio/notes",
  "version": "6.0.0",
  "description": "Note types manager",
  "repository": "",
  "main": "src/index.js",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./dist",
    "test-int": "stripes test nightmare --run demo --show",
    "test": "jest --coverage",
    "lint": "eslint ./ && stylelint \"src/**/*.css\"",
    "eslint": "eslint ./",
    "stylelint": "stylelint \"src/**/*.css\"",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-notes ./translations/ui-notes/compiled"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-react": "^7.10.4",
    "@folio/eslint-config-stripes": "^5.0.0",
    "@folio/stripes": "^7.0.0",
    "@folio/stripes-cli": "^2.0.0",
    "@formatjs/cli": "^4.2.7",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^11.0.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.3.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.2.1",
    "eslint-plugin-jest": "^24.1.3",
    "identity-obj-proxy": "^3.0.0",
    "inflected": "^2.0.4",
    "jest": "^26.4.2",
    "jest-junit": "^12.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.7.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0",
    "sinon": "^6.3.4",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-junit-formatter": "^0.2.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-notes.meta.title",
    "route": "/notes",
    "okapiInterfaces": {
      "notes": "2.0"
    },
    "queryResource": "query",
    "permissionSets": [
      {
        "permissionName": "module.notes.enabled",
        "displayName": "UI: ui-notes module is enabled",
        "visible": false
      },
      {
        "permissionName": "settings.notes.enabled",
        "displayName": "Settings (Notes): Edit and View General settings",
        "subPermissions": [
          "settings.enabled",
          "note.types.item.get",
          "note.types.collection.get",
          "note.types.item.post",
          "note.types.item.put",
          "note.types.item.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.view",
        "displayName": "Notes: Can view a note",
        "subPermissions": [
          "note.types.collection.get",
          "note.types.item.get",
          "notes.item.get",
          "notes.collection.get",
          "notes.collection.get.by.status",
          "notes.domain.all",
          "module.notes.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.create",
        "displayName": "Notes: Can create a note",
        "subPermissions": [
          "notes.item.post",
          "note.types.item.post",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.edit",
        "displayName": "Notes: Can edit a note",
        "subPermissions": [
          "notes.item.put",
          "note.types.item.put",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.delete",
        "displayName": "Notes: Can delete a note",
        "subPermissions": [
          "notes.item.delete",
          "note.types.item.delete",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.assign-unassign",
        "displayName": "Notes: Can assign and unassign a note",
        "subPermissions": [
          "note.links.collection.put",
          "ui-notes.item.view"
        ],
        "visible": true
      }
    ]
  }
}
