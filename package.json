{
  "name": "@folio/notes",
  "version": "1.3.0",
  "description": "Note types manager",
  "repository": "",
  "main": "src/index.js",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./dist",
    "test-int": "stripes test nightmare --run demo --show",
    "test": "stripes test karma",
    "lint": "eslint ./ && stylelint \"src/**/*.css\"",
    "eslint": "eslint ./",
    "stylelint": "stylelint \"src/**/*.css\""
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.8.1",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^2.7.4",
    "@folio/stripes-cli": "^1.12.1",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.6.0",
    "mocha": "^5.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "sinon": "^6.3.4",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-junit-formatter": "^0.2.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react-intl": "^2.9.0",
    "react-router-dom": "^4.1.1"
  },
  "peerDependencies": {
    "@folio/stripes": "^2.7.4",
    "react": "*"
  },
  "stripes": {
    "type": "settings",
    "displayName": "ui-notes.meta.title",
    "route": "/notes",
    "hasSettings": true,
    "okapiInterfaces": {
      "notes": "1.0"
    },
    "queryResource": "query",
    "permissionSets": [
      {
        "permissionName": "module.notes.enabled",
        "displayName": "UI: ui-notes module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.notes.enabled",
        "displayName": "Settings (notes): display list of settings pages",
        "subPermissions": [
          "settings.enabled",
          "note.types.allops"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.view",
        "displayName": "Notes: Can view a note",
        "subPermissions": [
          "note.types.collection.get",
          "notes.item.get",
          "notes.collection.get",
          "notes.collection.get.by.status",
          "notes.domain.all",
          "module.notes.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.create",
        "displayName": "Notes: Can create a note",
        "subPermissions": [
          "notes.item.post",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.edit",
        "displayName": "Notes: Can edit a note",
        "subPermissions": [
          "notes.item.put",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.delete",
        "displayName": "Notes: Can delete a note",
        "subPermissions": [
          "notes.item.delete",
          "notes.domain.all",
          "ui-notes.item.view"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-notes.item.assign-unassign",
        "displayName": "Notes: Can assign and unassign a note",
        "subPermissions": [
          "note.links.collection.put",
          "ui-notes.item.view"
        ],
        "visible": true
      }
    ]
  }
}
